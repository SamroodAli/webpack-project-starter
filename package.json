    "eslint-plugin-import": "^2.23.4",
    "hint": "^6.1.4",
    "sass": "^1.35.2",
    "sass-loader": "^12.1.0",
    "serve": "^12.0.0",
    "style-loader": "^3.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^21.0.0",
